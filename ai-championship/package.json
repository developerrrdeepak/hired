{
  "name": "ai-championship-hirevision",
  "version": "1.0.0",
  "description": "AI-powered recruitment platform with advanced candidate matching and interview automation",
  "private": true,
  "author": "AI Championship Team",
  "license": "MIT",
  "keywords": [
    "ai",
    "recruitment",
    "hiring",
    "nextjs",
    "typescript",
    "firebase"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-championship.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ai-championship/issues"
  },
  "homepage": "https://github.com/yourusername/ai-championship#readme",
  "scripts": {
    "dev": "next dev -p 9002",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "cross-env SKIP_ENV_VALIDATION=true next build",
    "export": "next build && next export",
    "deploy": "npm run build && firebase deploy --only hosting",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:emulators": "firebase emulators:start --only auth,firestore,storage",
    "test:integration": "cross-env NODE_ENV=test jest --testPathPattern=test/",
    "test:unit": "cross-env NODE_ENV=test jest --testPathPattern=src/",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "pre-commit": "npm run lint:check && npm run format:check && npm run typecheck && npm run test",
    "validate": "npm run security:audit && npm run pre-commit && npm run build"
  },
  "dependencies": {
    "@dataconnect/generated": "file:src/dataconnect-generated",
    "@genkit-ai/google-genai": "^1.20.0",
    "@genkit-ai/next": "^1.20.0",
    "@google/generative-ai": "^0.21.0",
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tanstack/react-table": "^8.19.3",
    "@vultr/vultr-node": "^2.8.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "elevenlabs": "^0.8.0",
    "embla-carousel-react": "^8.6.0",
    "express-rate-limit": "^7.1.0",
    "firebase": "^11.9.1",
    "genkit": "^1.20.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.475.0",
    "next": "^15.3.3",
    "patch-package": "^8.0.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.1",
    "stripe": "^14.13.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "validator": "^13.11.0",
    "wav": "^1.0.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/wav": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^10.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-security": "^1.7.1",
    "genkit-cli": "^1.20.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.0.0",
    "postcss": "^8",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5"
  }
}
